{
  "name": "color-blindless",
  "version": "1.0.2",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "color-blindless",
    "manifest": "src/manifest.json",
    "main": "color-blindless.sketchplugin",
